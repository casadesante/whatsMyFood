{
    "env": {
        "node": true, 
        "browser": true, 
        "es6": true 
    },
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "rules": {
      "react/jsx-filename-extension": "off",
      "no-console": "off",
      "react/jsx-one-expression-per-line": "off",
      "jsx-a11y/accessible-emoji": "off",
      "no-use-before-define": "off"
    }
}